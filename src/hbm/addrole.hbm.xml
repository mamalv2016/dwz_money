<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="money.addRole2.AddRoleVO" table="new_role">
		<id name="sno" column="id" type="int">
			<generator class="increment"></generator>
		</id>
				<property name="roleType" column="role_type" type="string" />
				<property name="roleKey" column="role_key" type="string" />
				<property name="roleIds" column="role_ids" type="string" />
				<property name="createUser" column="create_user" type="int" />
				<property name="createTime" column="create_time" type="string" />
				<property name="updateUser" column="update_user" type="int" />
				<property name="updateTime" column="update_time" type="string" />
	</class>

	<query name="money.addRole2.AddRoleDao.findRecordById">
		<![CDATA[from AddRoleVO as addRoleDao where sno = ?]]>
	</query>

	<query name="money.addRole2.AddRoleDao.findAll">
		<![CDATA[from AddRoleVO  ]]>
	</query>
</hibernate-mapping>