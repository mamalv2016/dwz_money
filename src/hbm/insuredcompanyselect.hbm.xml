<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ido.Dict_InsuredCompany.InsuredCompanySelectVO" table="dict_Insured_company">
		<id name="sno" column="id" type="int">
			<generator class="increment"></generator>
		</id>
				<property name="comName" column="com_name" type="string" />
				<property name="comStatus" column="com_status" type="string" />
				<property name="createUser" column="create_user" type="int" />
				<property name="createTime" column="create_time" type="string" />
				<property name="updateUser" column="update_user" type="int" />
				<property name="updateTime" column="update_time" type="string" />
	</class>

	<query name="ido.Dict_InsuredCompany.InsuredCompanySelectDao.findRecordById">
		<![CDATA[from InsuredCompanySelectVO as insuredCompanySelectDao where sno = ?]]>
	</query>
</hibernate-mapping>