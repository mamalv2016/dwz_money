<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ido.addmoneydetail.AddMoneyDetailVO" table="addmoney_record_detail">
		<id name="sno" column="id" type="int">
			<generator class="increment"></generator>
		</id>
				<property name="iuserId" column="iuser_id" type="string" />
				<property name="addType" column="add_type" type="string" />
				<property name="addMoney" column="Add_money" type="string" />
				<property name="insuredFileId" column="Insured_file_Id" type="string" />
				<property name="addTime" column="add_time" type="string" />
				<property name="createUser" column="create_user" type="int" />
				<property name="createTime" column="create_time" type="string" />
				<property name="updateUser" column="update_user" type="int" />
				<property name="updateTime" column="update_time" type="string" />
	</class>

	<query name="ido.addmoneydetail.AddMoneyDetailDao.findRecordById">
		<![CDATA[from AddMoneyDetailVO as addMoneyDetailDao where sno = ?]]>
	</query>

	<query name="ido.addmoneydetail.AddMoneyDetailDao.findAll">
		<![CDATA[from AddMoneyDetailVO  ]]>
	</query>
</hibernate-mapping>