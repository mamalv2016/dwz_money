<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ido.bindFamily.BindFamilyVO" table="bind_family">
		<id name="sno" column="id" type="int">
			<generator class="increment"></generator>
		</id>
				<property name="iuserNo" column="Iuser_no" type="string" />
				<property name="bindName" column="bind_name" type="string" />
				<property name="relation" column="relation" type="string" />
				<property name="cardNo" column="cardno" type="string" />
				<property name="phone" column="phone" type="string" />
				<property name="createUser" column="create_user" type="int" />
				<property name="createTime" column="create_time" type="string" />
	</class>

	<query name="ido.bindFamily.BindFamilyDao.findRecordById">
		<![CDATA[from BindFamilyVO as bindFamilyDao where sno = ?]]>
	</query>

	<query name="ido.bindFamily.BindFamilyDao.findAll">
		<![CDATA[from BindFamilyVO  ]]>
	</query>
</hibernate-mapping>