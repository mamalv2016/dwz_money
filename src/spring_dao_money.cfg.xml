			<value>money.detail.MoneyDao</value>
		</property>
		<property name="target">
			<bean parent="baseDao">
				<constructor-arg value="money.detail.MoneyVO">
				</constructor-arg>
			</bean>
		</property>
	</bean>

	<bean id="menuDao" parent="daoProxy">
