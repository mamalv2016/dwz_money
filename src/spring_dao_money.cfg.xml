			<value>money.question.QuestionDao</value>
		</property>
		<property name="target">
			<bean parent="baseDao">
				<constructor-arg value="money.question.QuestionVO">
				</constructor-arg>
			</bean>
		</property>
	</bean>

	<bean id="moneyDao" parent="daoProxy">
