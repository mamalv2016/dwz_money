		</property>
		<property name="target">
			<bean parent="baseDao">
				<constructor-arg value="money.paramtype.ParamTypeVO">
				</constructor-arg>
			</bean>
		</property>
	</bean>

	<bean id="myUserDao" parent="daoProxy">
		<property name="proxyInterfaces">
			<value>money.user.MyUserDao</value>
		</property>
		<property name="target">
			<bean parent="baseDao">
				<constructor-arg value="money.user.MyUserVO">
				</constructor-arg>
			</bean>
		</property>
	</bean>

	<bean id="paramDao" parent="daoProxy">
		<property name="proxyInterfaces">
