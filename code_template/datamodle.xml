<class name="YaopinInfo" table="chem_detail" desc="药品销售信息"
	package="money.yaopin">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		noedit="true" />
	<property name="companyName" column="companyName" type="string" querylike="true"
		desc="业务实体" width="100" notnull="true" query="true" />
	<property name="engName" column="eng_name" type="string" querylike="true"
		width="100" desc="物料名称（英文）" query="true" />
	<property name="chnName" column="chn_name" type="string" querylike="true"
		width="100" desc="物料名称（中文）" query="true" />
	<property name="chemStruct" column="chem_struct" type="string" query="true"
		desc="结构式"   width="100" />
	<property name="productName" column="product_name" type="string" query="true"
		desc="生产厂家" width="100" />
	<property name="customer" column="customer " type="string"
		width="100" desc="厂家销售客户"  />
	<property name="num" column="num" type="Double" desc="数量"
		width="100" />
	<property name="price" column="price" type="Double" width="100"
		desc="销售价格"  />
	<property name="chundu" column="chundu" type="string" width="100"
		desc="纯度" />
	<property name="saleTime" column="sale_time" type="date"  query="true"
		width="100" desc="销售时间" />
	<property name="cas" column="cas" desc="CAS" type="string" query="true"/>
	<property name="connect" column="connect" type="string" width="100"
		desc="厂家联系人" />
</class>