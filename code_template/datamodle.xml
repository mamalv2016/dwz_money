<class name="LoginUser" table="ido_user" desc="系统用户" importFile="true"
	cacheIdColumn="sno" cacheNameColumn="userName" cacheName="idouser_dict"
	exportFile="true" add="true" update="true" delete="true" package="ido.LoginUser">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		canExport="true" columnType="int(10)" noedit="true" />

	<property name="userName" column="user_Name" type="string"
		notNullInDb="true" canImport="true" canExport="true" maxLength="50"
		columnType="varchar(100)" desc="用户姓名" width="100" notnull="true"
		query="true" />

	<property name="userId" column="user_id" type="string"
		notnull="true" maxLength="100" columnType="varchar(100)" width="100"
		desc="登录名称 " query="true" />

	<property name="userType" column="user_type" type="string"
		selectCode="aiduyonghu" maxLength="100" columnType="varchar(100)"
		width="100" desc="所属类别 " showType="select" query="true" />

	<property name="userUnit" column="user_unit" type="string" noedit="true"
		notnull="true" query="true" queryLike="true" maxLength="100"
		columnType="varchar(100)" width="100" desc="所属单位 " />

	<property name="userPass" column="user_pass" type="string" visible="false"
		maxLength="100" columnType="varchar(100)" width="100" desc="用户密码" />

	<property name="userStatus" column="user_status" type="string"
		showType="select" selectCode="yesorno_status" columnType="int(10)"
		width="100" desc="用户状态 " query="true" />

	<property name="userPhone" column="user_phone" type="string"
		canImport="true" canExport="true" maxLength="20" columnType="varchar(50)"
		desc="联系电话" width="100" />

	<property name="userEmail" column="user_email" type="string"
		maxLength="200" columnType="varchar(200)" showType="email" width="100"
		desc="邮箱" />

	<property name="userAddress" column="user_address" type="string"
		rows="3" cols="40" columnType="varchar(1000)" showType="textarea"
		width="100" desc="地址" />

	<property name="createUser" column="create_user" type="int"
		currentUser="true" width="100" desc="创建用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="createTime" column="create_time" type="string"
		currentTime="true" width="100" desc="创建时间" visible="false" noedit="true"
		columnType="varchar(20)" noadd="true" />

	<property name="updateUser" column="update_user" type="int"
		currentUser="true" width="100" desc="更新用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="updateTime" column="update_time" type="string"
		currentTime="true" width="100" visible="false" noedit="true" desc="更新时间"
		columnType="varchar(20)" noadd="true" />
</class>