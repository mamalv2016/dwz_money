<class name="ContractInfo" table="crm_contract_info" desc="合同"
	package="eco.test">
	<id name="contractId" column="id" type="int" desc="流水号" width="10" visible="false" />
	<property name="ct_no" column="ct_no" type="varchar(50)" desc="合同编号"
		width="15%" notnull="true" query="true" />
	<property name="ct_type" column="ct_type" type="int" width="15%"
		desc="合作类别" />
	<property name="ct_money" column="ct_money" type="numeric(18,2)"
		desc="合同总额" width="15%" />
	<property name="ct_content" column="ct_content" type="varchar(500)"
		visible="false" width="15%" desc="咨询内容" query="true" />
	<property name="ct_start_date" column="ct_start_date" type="date" columnType="varchar(10)"
		desc="开始时间" width="15%" />
	<property name="ct_end_date" column="ct_end_date" type="date" columnType="varchar(10)"
		desc="结束时间" width="15%" />
	<property name="ct_travel" column="ct_travel" type="varchar(500)"
		visible="false" desc="差旅约定" query="true" />
	<property name="ct_pay_type" column="ct_pay_type" type="varchar(50)"
		visible="false" desc="支付方式" query="true" />
	<property name="ct_special" column="ct_special" type="varchar(2000)"
		visible="false" desc="特别条款说明" query="true" />
	<property name="shoukuan" column="shoukuan" type="varchar(100)"
		desc="收款情况" query="true" />
	<property name="ct_jingli" column="ct_jingli" type="varchar(50)"
		desc="项目负责人" query="true" />
	<property name="ct_zongjian" column="ct_zongjian" type="varchar(50)"
		visible="false" desc="项目总监" query="true" />
	<property name="ct_mishu" column="ct_mishu" type="varchar(50)"
		visible="false" desc="项目秘书" query="true" />
	<property name="ct_hexin" column="ct_hexin" type="varchar(100)"
		visible="false" desc="核心人员" query="true" />
	<property name="ct_crmId" column="crm_id" type="int" desc="关联客户"
		visible="false" />
</class>