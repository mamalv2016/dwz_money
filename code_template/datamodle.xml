<class name="YaopinInfo" table="yaopin" desc="药品销售信息"
	package="money.yaopin">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		noedit="true" />
	<property name="cas" column="cas" type="string" desc="订单"
		width="100" notnull="true" query="true" />
	<property name="engName" column="eng_name" type="string"
		width="100" desc="客户名称" query="true" />
	<property name="cheName" column="chn_name" type="string" desc="功率"
		notnull="true" width="100" />
	<property name="struct" column="struct" type="string" desc="电压"
		notnull="true" width="100" />
	<property name="chunDu" column="chundu " type="string" width="100"
		desc="世代" notnull="true" />
	<property name="produce" column="produce" type="string"
		desc="整流变压器厂家" width="100" />
	<property name="num" column="num" type="string"
		width="100" desc="整流变压器型号" query="true" />
	<property name="price" column="price" type="string"
		width="100" desc="是否重点客户" />
	<property name="connect" column="connect" type="date"
		width="100" desc="计划开工时间" notnull="true" />
	<property name="time" column="time" type="date" width="100"
		desc="计划结束时间" notnull="true" />
	<property name="customer" column="customer" type="string" selectType="orderstatus" 
		width="100" desc="当前状态" />
</class>