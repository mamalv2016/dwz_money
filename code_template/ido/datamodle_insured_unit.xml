<class name="InsuredUnit" table="Insured_unit" desc="投保单位"
	importFile="true" exportFile="false" add="true" update="false" delete="true"
	package="ido.InsuredUnit">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		canExport="true" columnType="int(10)" noedit="true" />

	<property name="unitCode" column="unit_code" type="string"
		notNullInDb="true" canImport="true" canExport="true" maxLength="250"
		columnType="varchar(500)" desc="编号" width="100" notnull="true" query="true"
		querylike="true" />

	<property name="unitName" column="unit_Name" type="string"
		notNullInDb="true" canImport="true" canExport="true" maxLength="500"
		columnType="varchar(1000)" width="100" desc="投保单位 " query="true"
		querylike="true" />

	<property name="contactName" column="contact_name" type="string"
		canImport="true" canExport="true" maxLength="50" columnType="varchar(100)"
		desc="联系人" width="100" />

	<property name="contactMobile" column="contact_mobile" type="string"
		canImport="true" canExport="true" maxLength="11" columnType="varchar(20)"
		showType="digits" desc="手机" width="100" />

	<property name="contactEmail" column="contact_email" type="string"
		canImport="true" canExport="true" maxLength="250" columnType="varchar(500)"
		showType="email" width="100" desc="邮箱" />

	<property name="unitParentId" column="unit_parent_id" type="int"
		canImport="true" canExport="true" columnType="int(10)" desc="上级单位" />

	<property name="unitState" column="unit_state" type="string"
		showType="select" selectCode="yesorno" canImport="true" canExport="true"
		columnType="int(10)" width="100" desc="是否显示" />

	<property name="unitAddress" column="unit_address" type="string"
		canImport="true" canExport="true" maxLength="250" cols="40" rows="4"
		columnType="varchar(500)" showType="textarea" width="100" desc="地址" />

	<property name="unitRemark" column="unit_remark" type="string"
		maxLength="2000" cols="40" rows="4" columnType="varchar(4000)"
		showType="textarea" width="100" desc="备注" />

	<property name="createUser" column="create_user" type="int"
		currentUser="true" width="100" desc="创建用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="createTime" column="create_time" type="string"
		currentTime="true" width="100" desc="创建时间" visible="false" noedit="true"
		columnType="varchar(20)" noadd="true" />

	<property name="updateUser" column="update_user" type="int"
		currentUser="true" width="100" desc="更新用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="updateTime" column="update_time" type="string"
		currentTime="true" width="100" visible="false" noedit="true" desc="更新时间"
		columnType="varchar(20)" noadd="true" />
</class>