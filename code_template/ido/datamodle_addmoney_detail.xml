<class name="AddMoneyDetail" table="addmoney_record_detail" desc="充值记录明细"
	importFile="true" exportFile="true" add="true" update="false" delete="true"
	package="ido.addmoneydetail">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		canExport="true" columnType="int(10)" noedit="true" />

	<property name="iuserId" column="iuser_id" type="string"
		notNullInDb="true" canImport="true" canExport="true" columnType="int(10)"
