	<property name="addTime" column="add_time" type="string"
		showType="date" canExport="true" columnType="varchar(20)" desc="充值时间"
		width="100" />

	<property name="createUser" column="create_user" type="int"
		canExport="true" currentUser="true" width="100" desc="创建用户" visible="false"
		noedit="true" columnType="int(10)" noadd="true" />

