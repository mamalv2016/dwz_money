<class name="BusinessShop" table="business_shop" desc="商铺"
	importFile="true" exportFile="false" add="true" update="false" delete="true"
	package="ido.BusinessShop">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		canExport="true" columnType="int(10)" noedit="true" />

	<property name="shopmId" column="Shopm_id" type="string"
		notNullInDb="true" canImport="true" canExport="true" maxLength="250"
		columnType="varchar(500)" desc="商家编号" width="100" notnull="true"
		query="true" />

	<property name="shopName" column="Shop_name" type="string" notnull="true"
		maxLength="250" notNullInDb="true" canImport="true" canExport="true"
		columnType="varchar(500)" width="100" desc="商铺名称 " query="true"
		querylike="true" />

	<property name="shopSno" column="shop_sno" type="string" notnull="true"
		maxLength="100" notNullInDb="true" canImport="true" canExport="true"
		columnType="varchar(200)" width="100" desc="商铺编号 " query="true"
		querylike="true" />

	<property name="shopStatus" column="shop_status" type="string"
		showType="select" selectCode="shopstatus" columnType="int(10)" width="100"
		desc="商铺状态 " query="true" />

	<property name="shopContactName" column="shop_contactname" notnull="true"
		type="string" canImport="true" canExport="true" maxLength="50"
		columnType="varchar(100)" desc="联系人名称" width="100" />

	<property name="shopConPhone" column="shop_contact_phone" notnull="true"
		type="string" canImport="true" canExport="true" maxLength="20"
		columnType="varchar(20)" desc="联系人手机" width="100" />
 
	<property name="shopEmail" column="shop_email" type="string" notnull="true"
		canImport="true" canExport="true" maxLength="250" columnType="varchar(500)"
		showType="email" width="100" desc="邮箱" />

	<property name="shopAddress" column="Shop_address" type="string" notnull="true"
		showType="textarea" rows="4" cols="40" canImport="true" canExport="true"
		columnType="varchar(500)" maxLength="250" width="100" desc="地址" /> 

	<property name="shopDate" column="Shop_date" type="string" notnull="true" showType="date"
		maxLength="100" columnType="varchar(20)" width="100" desc="签约日期" />

	<property name="shopJingdu" column="Shop_Jingdu" type="string" width="100"
		columnType="varchar(100)" maxLength="100" desc="经度" />

	<property name="shopWeidu" column="Shop_Weidu" type="string"
		columnType="Varchar(100)" maxLength="100" width="100" desc="纬度" />

	<property name="shopProvince" column="Shop_province" type="string"
		columnType="int(10)" idCoulmn="n_provid" nameColumn="s_provname" fromTable="dict_province"
		useCacheId="province_dict" showType="dict"  width="100"
		desc="省份" />

	<property name="shopCity" column="Shop_city" type="string"
		columnType="int(10)"  showType="dict" idCoulmn="n_cityid" nameColumn="s_cityname"
		fromTable="dict_city" useCacheId="city_dict"  width="100"
		desc="市" />

	<property name="shopxian" column="Shop_xian" type="string"
		columnType="int(10)" showType="select" selectCode="yesorno" width="100"
		desc="区县" />

	<property name="shopRemark" column="Shop_remark" type="string" visible="false"
		maxLength="1000" columnType="varchar(2000)" width="100" desc="备注" />

	<property name="shopMain" column="Shop_main" type="string" visible="false"
		maxLength="1000" columnType="varchar(2000)" width="100" desc="主营" />

	<property name="shopIntroduce" column="Shop_introduce" type="string" visible="false"
		maxLength="1000" columnType="varchar(2000)" width="100" desc="简介" />

	<property name="shopSpecial" column="Shop_special" type="string" visible="false"
		maxLength="1000" columnType="varchar(2000)" width="100" desc="特色" />

	<property name="createUser" column="create_user" type="int"
		currentUser="true" width="100" desc="创建用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="createTime" column="create_time" type="string"
		currentTime="true" width="100" desc="创建时间" visible="false" noedit="true"
		columnType="varchar(20)" noadd="true" />

	<property name="updateUser" column="update_user" type="int"
		currentUser="true" width="100" desc="更新用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="updateTime" column="update_time" type="string"
		currentTime="true" width="100" visible="false" noedit="true" desc="更新时间"
		columnType="varchar(20)" noadd="true" />
</class>