<class name="UserUpdateLogger" table="update_user_logger" desc="用户更新状态日志"
	importFile="true" exportFile="false" add="false" update="false" delete="true"
	package="ido.UserUpdateLogger">
	<id name="sno" column="id" type="int" desc="流水号" width="10" visible="false"
		canExport="true" columnType="int(10)" noedit="true" />

	<property name="userId" column="user_Id" type="string"
		notnull="true" notNullInDb="true" columnType="varchar(100)" width="250"
		desc="用户名 " query="true" />

	<property name="state" column="user_state" type="string"
		showType="select" selectCode="toubaouser_status" columnType="int"
		desc="更新状态" width="100" query="true" />

	<property name="logDetail" column="log_detail" type="string"
		maxLength="500" rows="3" cols="40"  
		showType="textarea" notnull="true" columnType="varchar(1000)" desc="操作原因"
		width="300" />

	<property name="arg1" column="arg1" type="string" visible="false"
		noedit="true" noadd="true" notnull="true" columnType="varchar(100)"
		desc="备用字段1" width="80" />

	<property name="createUser" column="create_user" type="int"
		currentUser="true" width="100" desc="创建用户" visible="false" noedit="true"
		columnType="int(10)" noadd="true" />

	<property name="createTime" column="create_time" type="string"
		currentTime="true" width="100" desc="创建时间" visible="false" noedit="true"
		columnType="varchar(20)" noadd="true" />

</class>